{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "trailingComma": "all",
                "semi": false
            }
        ],
        "react/prop-types": 0
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
